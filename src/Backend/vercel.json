{
    "builds": [
      {
        "src": "Backend/",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.10",
          "buildCommand": "pip install -r requirements.txt",
          "outputDirectory": "Backend/"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Backend/wsgi.py"
      }
    ]
  }
  